{
    "lines": 75,
    "statements": 75,
    "functions": 70,
    "branches": 60,
  "reporter": [
    "json",
    "text-summary"
  ],
  "exclude": [
    "coverage/**",
    "docs/**",
    "test{,s}/**",
    "test{,-*}.{js,cjs,mjs,ts}",
    "lib/browser/**",
    "package-scripts.js",
    "scripts/**"
  ],
  "check-coverage": true
}